<?xml version="1.0" encoding="utf-8" ?>
<application xmlns="http://ns.adobe.com/air/application/2.5">
    <id>compassapp.mutuallyhuman.com</id>
    <versionNumber>0.1.0</versionNumber>
    <filename>CompassApp-Test</filename>
    <name>CompassApp</name>
    <description>
      CompassApp is a cross platform, standalone Compass application for 
      designers to get started with using SASS.
    </description>

    <!-- for windows/linux program menus only, ignored on OSX -->
    <programMenuFolder>CompassApp</programMenuFolder>
    
    <icon>
      <image16x16>images/logo_16.png</image16x16>
      <image32x32>images/logo_32.png</image32x32>
      <image48x48>images/logo_48.png</image48x48>
      <image128x128>images/logo_512.png</image128x128>
    </icon>
    
    <initialWindow>
        <content>test.html</content>
        <visible>true</visible>
        <width>800</width>
        <height>600</height>
        <minSize>320 240</minSize>
    </initialWindow>
    <supportedProfiles>extendedDesktop</supportedProfiles>
</application>